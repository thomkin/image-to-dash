{
  "name": "@your-username/image-to-dash",
  "version": "1.0.0",
  "description": "A TypeScript library for processing images to DASH format using Podman",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "test": "cp ./podman_workflow.sh /tmp/workflow.sh && vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "packageManager": "pnpm@8.9.0",
  "keywords": [
    "dash",
    "podman",
    "typescript",
    "video-processing",
    "container"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/execa": "^2.0.2",
    "@types/fs-extra": "^11.0.3",
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "files": [
    "dist/**/*"
  ]
}
